<div class="container w-[18%] min-h-full float-left pt-10 pl-5" id="sidebar">
	<ul class="space-y-2 list-outside">

		<form action="/" method="get">
			<button class="p-2 w-48 text-left hover:bg-blue-300/70 border-l-8 border-gray-100 hover:border-blue-400 rounded-r-md rounded-l-lg ">
				<svg class="inline-block mr-4" xmlns="http://www.w3.org/2000/svg" height="24px" viewBox="0 0 24 24" width="20px" fill="#334155">
					<path d="M0 0h24v24H0V0z" fill="none" />
					<path d="M12 5.69l5 4.5V18h-2v-6H9v6H7v-7.81l5-4.5M12 3L2 12h3v8h6v-6h2v6h6v-8h3L12 3z" />
				</svg>
				<li class="inline-block text-slate-700 font-sofia text-sm">Home</li>
			</button>
		</form>

		<form action="/explore" method="get">
			<button class="p-2 w-48 text-left hover:bg-blue-300/70 border-l-8 border-gray-100 hover:border-blue-400 rounded-r-md rounded-l-lg ">
				<svg class="inline-block mr-4" xmlns="http://www.w3.org/2000/svg" height="24px" viewBox="0 0 24 24" width="20px" fill="#334155">
					<path d="M0 0h24v24H0V0z" fill="none" />
					<path d="M12 2C6.48 2 2 6.48 2 12s4.48 10 10 10 10-4.48 10-10S17.52 2 12 2zm0 18c-4.41 0-8-3.59-8-8s3.59-8 8-8 8 3.59 8 8-3.59 8-8 8zm-5.5-2.5l7.51-3.49L17.5 6.5 9.99 9.99 6.5 17.5zm5.5-6.6c.61 0 1.1.49 1.1 1.1s-.49 1.1-1.1 1.1-1.1-.49-1.1-1.1.49-1.1 1.1-1.1z" />
				</svg>
				<a href="/explore" class="inline-block text-slate-700 font-sofia text-sm">Explore Topics</a>
			</button>
		</form>


		 
		<form action="<% if(userSession && userSession.userid) { %>/user/<%= userSession.userid %>/question/createdQuestions<% } else { %>/user/notLoggedIn<% } %>" method="get">
			<button class="p-2 w-48 text-left hover:bg-blue-300/70 border-l-8 hover:border-blue-400 border-gray-100 rounded-r-md rounded-l-lg ">
				<svg class="inline-block mr-4" xmlns="http://www.w3.org/2000/svg" height="24px" viewBox="0 0 24 24" width="20px" fill="#334155">
					<path d="M0 0h24v24H0V0z" fill="none" />
					<path d="M11 18h2v-2h-2v2zm1-16C6.48 2 2 6.48 2 12s4.48 10 10 10 10-4.48 10-10S17.52 2 12 2zm0 18c-4.41 0-8-3.59-8-8s3.59-8 8-8 8 3.59 8 8-3.59 8-8 8zm0-14c-2.21 0-4 1.79-4 4h2c0-1.1.9-2 2-2s2 .9 2 2c0 2-3 1.75-3 5h2c0-2.25 3-2.5 3-5 0-2.21-1.79-4-4-4z" />
				</svg>
				<li class="inline-block text-slate-700 font-sofia text-sm">My Questions</li>
			</button>
		</form>


		<form action="<% if(userSession && userSession.userid) { %>/user/<%= userSession.userid %>/question/savedQuestions<% } else { %>/user/notLoggedIn<% } %>" method="get">
			
			<button class="p-2 w-48 text-left hover:bg-blue-300/70 border-l-8 border-gray-100 hover:border-blue-400 rounded-r-md rounded-l-lg ">
				<svg class="inline-block mr-4" xmlns="http://www.w3.org/2000/svg" height="24px" viewBox="0 0 24 24" width="20px" fill="#334155">
					<path d="M0 0h24v24H0V0z" fill="none" />
					<path d="M12 2C6.48 2 2 6.48 2 12s4.48 10 10 10 10-4.48 10-10S17.52 2 12 2zm0 18c-4.41 0-8-3.59-8-8s3.59-8 8-8 8 3.59 8 8-3.59 8-8 8zm-5.5-2.5l7.51-3.49L17.5 6.5 9.99 9.99 6.5 17.5zm5.5-6.6c.61 0 1.1.49 1.1 1.1s-.49 1.1-1.1 1.1-1.1-.49-1.1-1.1.49-1.1 1.1-1.1z" />
				</svg>
				<li class="inline-block text-slate-700 font-sofia text-sm">Saved Questions</li>
			</button>
		</form>

		<div class="group">
			
			<p class="p-2 w-48 text-left hover:bg-blue-300/70 border-l-8 border-gray-100 hover:border-blue-400 rounded-r-md rounded-l-lg font-sofia text-slate-700 text-sm"><svg xmlns="http://www.w3.org/2000/svg" class="mr-4 inline-block" fill="none" viewBox="0 0 24 24" stroke="currentColor" stroke-width="2" width="20px" height="24px">
				<path stroke-linecap="round" stroke-linejoin="round" d="M12 4.354a4 4 0 110 5.292M15 21H3v-1a6 6 0 0112 0v1zm0 0h6v-1a6 6 0 00-9-5.197M13 7a4 4 0 11-8 0 4 4 0 018 0z" />
			</svg>Followings</p>

			

			
			<form action="<% if (userSession && userSession.userid) { %>
				/user/<%= userSession.userid %>/getFollowedUsers
			<% } else {%>
					/user/notLoggedIn
				<% } %> " method="get" class="text-left">
				<button class="p-2 w-36 text-left hover:bg-blue-300/70 border-l-8 border-gray-100 hover:border-blue-400 rounded-r-md rounded-l-lg ">
					<li class="inline-block text-slate-700 font-sofia text-sm ">Followed Section</li>
	
				</button>
			</form>

			<form action="<% if (userSession && userSession.userid) { %>
				/user/<%= userSession.userid %>/getFollowedUsersQuestions
			<% } else {%>
					/user/notLoggedIn
				<% } %> " method="get" class="text-left">
				<button class="p-2 w-48 text-left hover:bg-blue-300/70 border-l-8 border-gray-100 hover:border-blue-400 rounded-r-md rounded-l-lg ">
					<li class="inline-block text-slate-700 font-sofia text-sm">Followed Users Questions</li>
				</button>
			</form>
		</div>


				

		

		

		
	</ul>
</div>

